{
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 18,
    "editor.tabSize": 5,
    "editor.cursorStyle": "line-thin",
    "editor.wordWrap": "on",
    "editor.accessibilityPageSize": 15,
